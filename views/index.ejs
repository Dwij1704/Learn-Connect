    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/2.1.2/sweetalert.min.js"></script>
    <script>
      $(document).ready(function () {
        // Show the registration modal when "Register" link is clicked
        $('a[href="#et-register-wrap"]').click(function () {
          $("#login").modal("hide"); // Hide login modal if open
          $("#et-register-wrap").modal("show");
        });
        var urlParams = new URLSearchParams(window.location.search);
        if (urlParams.get("success")=='true') {
          swal({
            title: "Successful",
            text: `${urlParams.get("message")}`,
            icon: "success",
            button: "Continue",
          }).then(() => {
            console.log(window.location.hostname);
            window.location.replace(window.location.origin + "");
          });
        }
        else if (urlParams.get("success") == 'false') {
          swal({
            title: "Failed",
            text: `${urlParams.get("message")}`,
            icon: "error",
            button: "Do it Again!",
          }).then(() => {
            console.log(window.location.hostname);
            window.location.replace(window.location.origin + "");
          });
        }
		const form = document.getElementById("myForm");
		form.addEventListener("submit", function (e) {
		e.preventDefault(); // Prevent the form from submitting

		const password = form.querySelector("[name='pwd']").value;
		const confirmPassword = form.querySelector("[name='cpwd']").value;

		if (password !== confirmPassword) {
			// Passwords don't match
			swal({
			icon: "error",
			title: "Passwords do not match",
			text: "Please make sure your passwords match.",
      button: "Okay",
			});
		} else {
			// Passwords match, submit the form
			form.submit();
		}
		});
      });
    </script>
